                params: {
                    indicador: 'VER'
                }
            });
        };

        function eventoBandejaDispensacion(codigo , fechacreacion) {

            console.log(codigo);
            console.log(fechacreacion);

            Ext.net.DirectMethod.request({
                url: '@(Url.Action("eventoDispensacion"))',
                params: {
                    codigoOA: codigo,
                    fechacreacion: fechacreacion
                }
            });
        };





