    <script>


        function EventoSalirSyss() {
            Ext.net.DirectMethod.request({
                url: '@(Url.Action("eventoSalirSistemas"))',
                params: {
                    indicador: 'SALIR'
                }
            });
        };

        function eventoPassword() {
            Ext.net.DirectMethod.request({
                url: '@(Url.Action("eventoPassword"))',
                params: {
                    indicador: 'VER'
                }
            });
        };


        function eventoAbout() {
            Ext.net.DirectMethod.request({
